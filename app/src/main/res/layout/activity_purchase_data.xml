                    style="@style/oaTextView2"
                    android:layout_gravity="center_vertical"
                    android:text="总经理:" />

                <View
                    android:layout_width="1dp"
                    android:layout_height="15dp"
                    android:layout_gravity="center_vertical"
                    android:background="@color/shouye" />

                <EditText
                    android:id="@+id/etLeader5"
                    style="@style/textWrite2"
